#include <check.h>
#include <math.h>
#include <stdio.h>

#include "s21_math.h"

#test s21_log_test
for (double i = 0.01; i <= 1000; i += 0.1) {
  long double s21_res = s21_log(i);
  long double res = log(i);
  ck_assert_double_eq_tol(res, s21_res, S21_EPSILON);
}

#test s21_log_test_zero
ck_assert_double_eq(logl(0), s21_log(0));
