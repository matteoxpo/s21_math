#include <check.h>
#include <math.h>

#include "s21_math.h"

#test s21_acos_test
for (double i = -1; i <= 1; i += 0.1) {
  ck_assert_double_eq_tol(acosl(i), s21_acos(i), S21_EPSILON * 10);
}
ck_assert_ldouble_nan(s21_acos(NAN));
ck_assert_double_nan(s21_acos(INFINITY));
ck_assert_double_nan(s21_acos(-INFINITY));
