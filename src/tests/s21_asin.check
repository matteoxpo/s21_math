#include <check.h>
#include <math.h>

#include "s21_math.h"

#test s21_asin_test
for (double i = -1; i <= 1; i += 0.1) {
  ck_assert_double_eq_tol(asinl(i), s21_asin(i), S21_EPSILON);
}
ck_assert_ldouble_nan(s21_asin(NAN));
ck_assert_double_nan(s21_asin(INFINITY));
ck_assert_double_nan(s21_asin(-INFINITY));